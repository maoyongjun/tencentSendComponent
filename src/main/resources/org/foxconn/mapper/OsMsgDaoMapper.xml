<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
 <mapper namespace="org.foxconn.tencent.sendComponent.dao.OsMsgDao">
 
 	<select id="findAll"  parameterType="map" statementType="CALLABLE" resultType="org.foxconn.tencent.sendComponent.entity.OsTestResultJsonModel">
 		<![CDATA[
		    {call efoxsfcgetTencentShipoutExcelMsg
		     (#{ssn,mode=IN,jdbcType=VARCHAR},
		      #{startTime,mode=IN,jdbcType=DATE},
		      #{endTime,mode=IN,jdbcType=DATE}
		     )
		     }
		]]>
 	</select>
 	
 	<select id="getTestResult"  statementType="CALLABLE" resultType="org.foxconn.tencent.sendComponent.entity.OsTestResultJsonModel">
 		<![CDATA[
		    {call eFoxSFCMarkSNParsed
		     ()
		     }
		]]>
 	</select>
 	
 	<insert id="addComponent" parameterType="org.foxconn.tencent.sendComponent.entity.Component">
 		INSERT INTO efoxSFCTencentComponent(ID,SN,PN,FW,CTYPE,PARENTID,lasteditdt) values (#{id},#{sn},#{pn},#{fw},#{type},#{parentId},#{lasteditdt})
 	</insert>
 	
 </mapper>        
 